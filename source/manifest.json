{
	"name": "Awesome Extension ABC",
	"version": "0.0.0",
	"description": "An awesome new browser extension",
	"homepage_url": "https://github.com/fregante/browser-extension-template",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"<all_urls>"
	],
	"browser_action": {
	"default_icon": "icon.png",
	"theme_icons": [{
		"light": "icon.png",
		"dark": "icon.png",
		"size": 32
	}],
	"default_title": "Orders",
	"default_popup": "popup/orders.html"
	},
	"options_ui": {
		"chrome_style": true,
		"page": "options/options.html"
	}
}
